<h1>Liste de tous les films</h1>
<% @lists.each do |list| %>
  <h2><%= list.name %></h2>
  <%= link_to "Voir la liste", list_path(list) %>
  <% if list.photo.attached? %>
  <%= cl_image_tag list.photo.key, height: 300, crop: :fill %>
  <% end %>
<% end %>

